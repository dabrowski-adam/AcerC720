# Standard
pcm.!default {
  type hw
  card 0 
  device 0
}

ctl.!default {
  type hw
  card 0 
}

pcm_slave.slavej {
  pcm "hw:0"
  channels 2
  rate 44100
}

pcm.plugj {
  type plug
  slave slavej
}

